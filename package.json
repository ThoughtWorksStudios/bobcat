{
  "name": "datagen",
  "version": "1.0.0",
  "description": "Prototype JSON generator",
  "main": "index.js",
  "dependencies": {
    "dashdash": "^1.14.1",
    "dateformat": "^2.0.0",
    "faker": "^4.1.0",
    "hyperid": "^1.2.0",
    "lodash": "^4.17.4",
    "pegjs": "^0.10.0",
    "pkg": "^4.2.2"
  },
  "devDependencies": {
    "tap-difflet": "^0.7.0",
    "tape": "^4.8.0"
  },
  "scripts": {
    "clean": "rm -rf panther lib/dsl.js node_modules; find . -type f ! -name 'package*.json' -name '*.json' -delete",
    "build": "node_modules/pegjs/bin/pegjs -o lib/dsl.js grammar.pegjs && node_modules/pkg/lib-es5/bin.js -t node8-linux,node8-macos -o panther index.js",
    "test": "node_modules/tape/bin/tape test/*-test.js | node_modules/tap-difflet/bin/tap-difflet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThoughtWorksStudios/datagen.git"
  },
  "author": "ThoughtWorks Studios",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ThoughtWorksStudios/datagen/issues"
  },
  "homepage": "https://github.com/ThoughtWorksStudios/datagen#readme"
}
